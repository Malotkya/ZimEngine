{
    "name": "zim-engine",
    "version": "0.7.0",
    "description": "Combining front and backend rendering",
    "main": "lib/index.js",
    "scripts": {
        "build:dev": "tsc --sourceMap && webpack-cli --env dev",
        "build:prod": "rimraf lib && tsc && webpack-cli --env prod",
        "test": "npm run build:dev && jest",
        "test:serve": "npm run build:dev && node ./test",
        "deploy": "npm run build:prod && jest && npm publish"
    },
    "author": "Alex Malotky",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^20.11.30",
        "jest": "^29.7.0",
        "rimraf": "^6.0.1",
        "supertest": "^6.3.4",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.3"
    },
    "dependencies": {
        "path-to-regexp": "^8.2.0"
    },
    "optionalDependencies": {
        "json5": "^2.2.3",
        "terser-webpack-plugin": "^5.3.10",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Malotkya/ZimEngine"
    },
    "exports": {
        "./HttpError": {
            "import": "./lib/HttpError.js",
            "types": "./lib/HttpError.d.ts"
        },
        "./Static": {
            "import": "./lib/Static.js",
            "types": "./lib/Static.d.ts"
        },
        ".": {
            "import": "./lib/index.js",
            "types": "./lib/index.d.ts"
        }
    },
    "files": [
        "lib",
        "webpack"
    ]
}
